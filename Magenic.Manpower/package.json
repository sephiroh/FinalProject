    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "protractor": "~4.0.14"
  },
  "dependencies": {
    "angular": "^1.6.1",
    "angular-animate": "^1.6.1",
    "angular-cookies": "^1.6.3",
    "angular-ui-router": "^0.4.2",
    "angularjs-toaster": "^2.1.0",
    "bootstrap": "^3.3.7",
    "jquery": "^1.7.4",
    "requirejs": "^2.3.2"
